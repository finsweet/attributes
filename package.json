{
  "name": "@finsweet/attributes",
  "description": "Finsweet's Attributes monorepo.",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "bugs": {
    "url": "https://github.com/finsweet/attributes/issues"
  },
  "homepage": "https://github.com/finsweet/attributes#readme",
  "scripts": {
    "build": "pnpm -r --filter \"./packages/**\" build",
    "lint": "pnpm -r --filter \"./packages/**\" lint",
    "check": "pnpm -r --filter \"./packages/**\" check",
    "format": "pnpm -r --filter \"./packages/**\" format",
    "test": "pnpm -r --filter \"./packages/**\" --filter !@finsweet/attributes-support test --workspace-concurrency=1",
    "test:headed": "pnpm -r --filter \"./packages/**\" --filter !@finsweet/attributes-support test:headed --workspace-concurrency=1",
    "release": "changeset publish",
    "update": "pnpm update -i -L -r",
    "serve": "serve"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.1.11",
    "@changesets/cli": "^2.22.0",
    "@finsweet/eslint-config": "^1.1.4",
    "@finsweet/tsconfig": "^1.1.0",
    "@playwright/test": "^1.22.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.43",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "serve": "^13.0.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.33.2"
  }
}
