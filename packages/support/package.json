{
  "name": "@finsweet/attributes-support",
  "version": "1.1.0",
  "files": [
    "support.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "jest",
    "test:e2e": "testcafe \"chrome:headless '--window-size=1920,1080'\" tests ",
    "test:e2e:browser": "testcafe \"chrome '--window-size=1920,1080'\" tests",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**/*.{js,ts,svelte}",
    "lint:fix": "eslint src/**/*.{js,ts,svelte} --fix",
    "tsc": "tsc --noEmit",
    "format": "prettier --write ./src"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@rollup/plugin-url": "^7.0.0",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.4.7",
    "@sveltejs/eslint-config": "sveltejs/eslint-config",
    "@tsconfig/svelte": "^2.0.0",
    "@types/jest": "^27.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.5",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.2",
    "svelte-check": "^2.0.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.0.0",
    "testcafe": "^1.17.1",
    "tslib": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@testing-library/jest-dom": "^5.16.1",
    "jest": "^27.4.5",
    "jscpd": "^3.4.2",
    "rollup-plugin-copy": "^3.4.0",
    "sirv-cli": "^1.0.0",
    "svelte-jester": "^2.1.5",
    "ts-jest": "^27.1.2"
  }
}
