{
  "name": "@finsweet/attributes-shopify",
  "version": "1.0.0",
  "description": "Integrate Shopify's Buy SDK in no-code using Attributes.",
  "homepage": "https://www.finsweet.com/attributes",
  "sideEffects": false,
  "type": "module",
  "main": "shopify.js",
  "files": [
    "shopify.js",
    "examples.json",
    "schema.json"
  ],
  "scripts": {
    "lint": "eslint ./src && prettier --check ./src",
    "lint:fix": "eslint ./src --fix && prettier --write ./src",
    "check": "tsc --noEmit",
    "test": "pnpm playwright test",
    "test:headed": "pnpm playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "keywords": [
    "webflow",
    "attributes",
    "no code",
    "shopify"
  ],
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "bugs": {
    "url": "https://sweetjs.finsweet.com/"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./schema": {
      "types": "./src/utils/schema.ts",
      "import": "./src/utils/schema.ts"
    }
  },
  "devDependencies": {
    "@types/shopify-buy": "^2.10.10"
  },
  "dependencies": {
    "@finsweet/attributes-utils": "workspace:*",
    "shopify-buy": "^2.18.0"
  }
}
