{
  "name": "@finsweet/attributes-richtext",
  "version": "1.6.0",
  "description": "Add HTML to Rich Text elements, add components from the same page or from different pages, add classes, ids, attributes, and more.",
  "homepage": "https://www.finsweet.com/attributes",
  "sideEffects": false,
  "type": "module",
  "main": "richtext.js",
  "files": [
    "richtext.js",
    "examples.json",
    "schema.json",
    "changesets.json"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./bin/build.js",
    "build": "cross-env NODE_ENV=production node ./bin/build.js",
    "lint": "eslint ./src && prettier --check ./src",
    "check": "tsc --noEmit",
    "format": "prettier --write ./src",
    "test": "pnpm playwright test",
    "test:headed": "pnpm playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "keywords": [
    "webflow",
    "attributes",
    "no code",
    "rich text block",
    "rtb"
  ],
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "bugs": {
    "url": "https://sweetjs.finsweet.com/"
  },
  "devDependencies": {
    "@global/build": "workspace:*",
    "@global/tests": "workspace:*",
    "@global/types": "workspace:*",
    "@types/dompurify": "^2.3.1"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.33.1",
    "@global/constants": "workspace:*",
    "@global/factory": "workspace:*"
  }
}
