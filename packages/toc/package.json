{
  "name": "@finsweet/attributes-toc",
  "version": "1.1.0",
  "description": "Automatically generate a table of contents based on heading tags. Works with Webflow Rich Text, Webflow \"Current\" class, and anchor scroll. CMS, static, and all text elements are supported.",
  "homepage": "https://www.finsweet.com/attributes",
  "sideEffects": false,
  "type": "module",
  "main": "toc.js",
  "files": [
    "toc.js",
    "examples.json",
    "schema.json",
    "changesets.json"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./bin/build.js",
    "build": "cross-env NODE_ENV=production node ./bin/build.js",
    "lint": "eslint ./src && prettier --check ./src",
    "check": "tsc --noEmit",
    "format": "prettier --write ./src",
    "test": "pnpm playwright test",
    "test:headed": "pnpm playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "keywords": [
    "webflow",
    "attributes",
    "no code",
    "table of contents",
    "toc",
    "rich text"
  ],
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "bugs": {
    "url": "https://sweetjs.finsweet.com/"
  },
  "devDependencies": {
    "@global/build": "workspace:*",
    "@global/tests": "workspace:*",
    "@global/types": "workspace:*"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.33.1",
    "@global/constants": "workspace:*",
    "@global/factory": "workspace:*",
    "@global/helpers": "workspace:*",
    "slugify": "^1.6.5"
  }
}
