{
  "name": "@finsweet/attributes-smartlightbox",
  "version": "1.6.6",
  "description": "Change the position of an opened lightbox to prevent unwanted `z-index`, `fixed` positioning, and `transform` issues.",
  "homepage": "https://www.finsweet.com/attributes",
  "sideEffects": false,
  "type": "module",
  "main": "smartlightbox.js",
  "files": [
    "smartlightbox.js",
    "examples.json",
    "schema.json",
    "changesets.json"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./bin/build.js",
    "build": "cross-env NODE_ENV=production node ./bin/build.js",
    "lint": "eslint ./src && prettier --check ./src",
    "check": "tsc --noEmit",
    "format": "prettier --write ./src",
    "test": "pnpm playwright test",
    "test:headed": "pnpm playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "keywords": [
    "webflow",
    "fixed",
    "lightbox",
    "untransform",
    "attributes",
    "no code"
  ],
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "bugs": {
    "url": "https://sweetjs.finsweet.com/"
  }
}
