{
  "name": "@finsweet/attributes-docs",
  "version": "1.12.11",
  "description": "Attributes documentation custom code.",
  "homepage": "https://www.finsweet.com/attributes",
  "sideEffects": false,
  "type": "module",
  "main": "docs.js",
  "files": [
    "docs.js",
    "changelog.js",
    "api.js",
    "support.js",
    "attributes.json",
    "changesets.json",
    "examples.json"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./bin/build.js",
    "build:dev": "cross-env NODE_ENV=test node ./bin/build.js",
    "build": "cross-env NODE_ENV=production node ./bin/build.js",
    "lint": "eslint ./src && prettier --check ./src",
    "lint:fix": "eslint ./src --fix && prettier --write ./src",
    "check": "tsc --noEmit",
    "format": "prettier --write ./src",
    "test": "pnpm playwright test",
    "test:headed": "pnpm playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "devDependencies": {
    "@types/marked": "^5.0.0"
  },
  "dependencies": {
    "marked": "^5.1.0",
    "@finsweet/attributes-utils": "workspace:*"
  }
}
